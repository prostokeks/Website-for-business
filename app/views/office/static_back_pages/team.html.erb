<section class="py-6 dark:bg-gray-800 dark:text-gray-100 mt-12">
  <div class="container flex flex-col items-center justify-center p-4 mx-auto space-y-8 sm:p-10">
    <h1 class="text-4xl font-bold leading-none text-center sm:text-5xl mb-24">Our team</h1>
    <h3 class="font-bold leading-none text-center text-2xl">Project Manager</h3>
    <div class="flex flex-row flex-wrap-reverse justify-center">
      <% @userspm.each do |u| %>
        <%= link_to (office_user_profile_path(u.id)) do %>
          <div class="flex flex-col justify-center m-8 text-center">
            <% if u.avatar.attached? %>
              <%= image_tag u.avatar, class: "self-center flex-shrink-0 w-24 h-24 mb-4 bg-center bg-cover rounded-full dark:bg-gray-500"%>
            <% else %>
              <img class="self-center flex-shrink-0 w-24 h-24 mb-4 bg-center bg-cover rounded-full dark:bg-gray-500" src="https://st2.depositphotos.com/47577860/46283/v/450/depositphotos_462836132-stock-illustration-assassin-deadpool-face-icon.jpg" alt="">
            <% end %>
            <p class="text-xl font-semibold leading-tight"><%= u.name %> <%= u.last_name %></p>
            <p class="dark:text-gray-400"><%= u.role %></p>
          </div>
        <% end %>
      <% end %>
    </div>
